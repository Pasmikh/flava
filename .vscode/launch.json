    "version": "0.2.0",
    "configurations": [

        {
            "name": "flava",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flava (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flava (web html)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-renderer",
                "html",
            ]
        },
        {
            "name": "flava (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
